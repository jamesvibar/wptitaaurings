{% extends "base.twig" %}

{% block content %}
  <section id="contact" class="section container">
  
    <div class="row justify-content-center">
      <div class="col-md-6">
        {{ post.content | shortcodes }}
      </div>

      <div class="col-auto offset-md-1">
        <div class="contact-main">
        
          <img class="contact__logo" src="{{ theme.link | e("esc_url") | relative }}/static/images/logo-colored.png">
          <p>The Queen of Chicken Wings in Cavite</p>

          <ul class="contact-social">
            <li><i class="fa fas fa-map-marker-alt mr-2"></i> 24 Sotto St., General Trias, Cavite</li>
            <li><i class="fa fas fa-envelope mr-2"></i>ordernow@titaaurings.com.ph</li>
            <li><i class="fa fas fa-phone mr-2"></i>(+63) 905 669 5251</li>
            <li><i class="fa fab fa-facebook-messenger mr-2"></i>m.me/titaaurings</li>
          </ul>

        </div>
      </div>
    </div>

  
  </section>
{% endblock %}